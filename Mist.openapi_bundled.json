{"enabled": {"type": "boolean"}, "allowed_subnets": {"type": "array", "description": "list of CIDRs", "items": {"type": "string"}}, "allowed_hostnames": {"type": "array", "description": "list of hostnames without http(s):// (matched by substring)", "items": {"type": "string"}}}, "required": ["enabled", "allowed_subnets", "allowed_hostnames"], "title": ""}, "wlan_datarates": {"description": "data rates wlan settings", "type": "object", "properties": {"min_rssi": {"type": "integer", "description": "Minimum RSSI for client to connect, 0 means not enforcing"}, "template": {"type": "string", "minLength": 1, "description": "* no-legacy: no 11b\n* compatible: all, like before, default setting that Broadcom/Atheros used\n* legacy-only: disable 802.11n and 802.11ac \n* high-density: no 11b, no low rates\n* custom: user defined"}, "legacy": {"type": "array", "description": "list of supported rates (IE=1) and extended supported rates (IE=50), append \u2018b\u2019 at the end to indicate it being basic/mandatory", "items": {"type": "string"}}, "ht": {"type": "string", "minLength": 1, "description": "MCS bitmasks for 4 streams (16-bit for each stream, MCS0 is least significant bit), e.g. 00ff 00f0 001f limits HT rates to MCS 0-7 for 1 stream, MCS 4-7 for 2 stream (i.e. MCS 12-15), MCS 1-5 for 3 stream (i.e. MCS 16-20)"}, "vht": {"type": "string", "description": "MCS bitmasks for 4 streams (16-bit for each stream, MCS0 is least significant bit), e.g. 03ff 01ff 00ff limits VHT rates to MCS 0-9 for 1 stream, MCS 0-8 for 2 streams, and MCS 0-7 for 3 streams."}}}, "wlan_bonjour": {"description": "bonjour gateway wlan settings", "type": "object", "properties": {"enabled": {"type": "boolean", "description": "whether to enable bonjour for this WLAN, default is false. Once enabled, limit_bcast is assumed true, allow_mdns is assumed false"}, "services": {"type": "object", "description": "what services are allowed", "additionalProperties": {"type": "object", "properties": {"scope": {"type": "string", "description": "how bonjour services should be discovered for the same WLAN, same_site (default) / same_map / same_ap"}, "disable_local": {"type": "boolean", "description": "whether to prevent wireless clients to discover bonjour devices on the same WLAN, default is false"}, "radius_groups": {"type": "array", "description": "optional, if the service is further restricted for certain RADIUS groups", "items": {"type": "string"}}}}}, "additional_vlan_ids": {"type": "array", "description": "additional VLAN IDs (on the LAN side or from other WLANs) should we be forwarding bonjour queries/responses", "items": {"type": "integer"}}}, "required": ["services", "additional_vlan_ids"], "x-examples": {"example-1": {"enabled": true, "services": {"airprint": {"scope": "same_map"}, "airplay": {"scope": "same_ap", "radius_groups": ["teachers"]}}, "additional_vlan_ids": [80]}}}}}}
